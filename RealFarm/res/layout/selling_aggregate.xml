                    android:layout_centerVertical="true"
                    android:layout_margin="5dp"
                    android:background="@drawable/circular_btn"
                    android:src="@drawable/ic_48px_thumbs_up" />

                 </RelativeLayout>
            </TableRow>
        </LinearLayout>

        
