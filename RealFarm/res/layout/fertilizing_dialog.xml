<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/linearLayout19"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/header" >

        <ImageView
            android:id="@+id/aggr_img_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/circular_btn_normal"
            android:src="@drawable/ic_72px_fertilizing1" />

        <ImageView
            android:id="@+id/aggr_img_icon"
            android:layout_width="60dip"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:src="@drawable/plot" />

        <Button
            android:id="@+id/button123"
            android:layout_width="75dip"
            android:layout_height="wrap_content"
            android:text="Button"
            android:visibility="invisible" />

        <ImageButton
            android:id="@+id/aggr_img_home"
            android:layout_width="50dip"
            android:layout_height="match_parent"
            android:layout_margin="3dp"
            android:background="@drawable/home"
            android:src="@drawable/square_btn_transp" />

        <ImageButton
            android:id="@+id/aggr_img_help"
            android:layout_width="50dip"
            android:layout_height="match_parent"
            android:layout_margin="3dp"
            android:background="@drawable/help"
            android:src="@drawable/square_btn_transp" />

    </LinearLayout>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="70dp"
        android:orientation="horizontal" >

        <TableRow
            android:id="@+id/var_fert_tr"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:background="@drawable/def_img"
            android:paddingBottom="10dip"
            android:paddingTop="4dip" >

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_margin="5dip"
                android:gravity="right" >

                <Button
                    android:id="@+id/variety_sow_txt_btn"
                    android:layout_width="120dp"
                    android:layout_height="60dp"
                    android:layout_alignBottom="@+id/dlg_var_sow"
                    android:layout_alignParentLeft="true"
                    android:layout_alignTop="@+id/dlg_var_sow"
                    android:background="@drawable/square_btn_transp" />

                <TextView
                    android:id="@+id/variety_sow_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignRight="@+id/variety_sow_txt_btn"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="Fertilizer Name"
                    android:textColor="#000000"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="40dp"
                android:layout_height="60dp"
                android:layout_margin="5dip"
                android:gravity="right" >

                <ImageButton
                    android:id="@+id/variety_sow_img_btn"
                    android:layout_width="40dip"
                    android:layout_height="60dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/square_btn_transp"
                    android:gravity="center"
                    android:src="@drawable/ic_48px_fertilizing2" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_margin="3dip"
                android:gravity="right" >

                <ImageView
                    android:id="@+id/img_bg_var_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/empty" />

                <TextView
                    android:id="@+id/dlg_lbl_var_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_weight="1"
                    android:gravity="right"
                     android:layout_marginLeft="5dp"
                    android:text="Fertilizer"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="21sp" />

                <Button
                    android:id="@+id/home_btn_var_fert"
                    android:layout_width="120dp"
                    android:layout_height="60dp"
                    android:layout_alignBottom="@+id/dlg_var_sow"
                    android:layout_alignParentLeft="true"
                    android:layout_alignTop="@+id/dlg_var_sow"
                    android:background="@drawable/square_btn_transp" />
            </RelativeLayout>
        </TableRow>
    </LinearLayout>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="70dp"
        android:orientation="horizontal" >

        <TableRow
            android:id="@+id/units_fert_tr"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:background="@drawable/def_img"
            android:paddingBottom="10dip"
            android:paddingTop="4dip" >

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_margin="5dip"
                android:gravity="right" >

                <TextView
                    android:id="@+id/amount_sow_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="Amount"
                    android:textColor="#000000"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/amount_sow_txt_btn"
                    android:layout_width="120dp"
                    android:layout_height="60dp"
                    android:layout_alignBottom="@+id/dlg_var_sow"
                    android:layout_alignParentLeft="true"
                    android:layout_alignTop="@+id/dlg_var_sow"
                    android:background="@drawable/square_btn_transp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="40dp"
                android:layout_height="60dp"
                android:layout_margin="5dip"
                android:gravity="right" >

                <ImageButton
                    android:id="@+id/amount_sow_img_btn"
                    android:layout_width="40dip"
                    android:layout_height="60dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:background="@drawable/square_btn_transp"
                    android:gravity="center"
                    android:src="@drawable/ic_48px_pest" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.09" >

                <ImageView
                    android:id="@+id/img_bg_units_no_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/empty" />
                
                  <TextView
                    android:id="@+id/dlg_lbl_unit_no_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignRight="@+id/dlg_unit_sow"
                    android:layout_centerVertical="true"
                    android:paddingBottom="5dip"
                    android:paddingLeft="5dp"
                    android:paddingTop="5dip"
                    android:text="No"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <Button
                    android:id="@+id/home_btn_units_no_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignRight="@+id/img_bg_units_no_fert"
                    android:background="@drawable/square_btn_transp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:layout_weight="0.09"
                android:paddingLeft="10dp"
                android:paddingRight="12dp" >

                <ImageView
                    android:id="@+id/img_bg_units_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    
                    android:src="@drawable/empty" />

                <TextView
                    android:id="@+id/dlg_lbl_units_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingBottom="5dip"
                    android:paddingLeft="15dp"
                    android:paddingTop="5dip"
                    android:text="Kg's"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <Button
                    android:id="@+id/home_btn_units_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignTop="@+id/dlg_lbl_units_fert"
                    android:background="@drawable/square_btn_transp" />
                
            </RelativeLayout>
        </TableRow>
    </LinearLayout>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="70dp"
        android:orientation="horizontal" >

        <TableRow
            android:id="@+id/day_fert_tr"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:background="@drawable/def_img"
            android:paddingBottom="10dip"
            android:paddingTop="4dip" >

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_margin="5dip"
                android:gravity="right" >

                <TextView
                    android:id="@+id/date_sow_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="Date"
                    android:textColor="#000000"
                    android:textSize="20sp" />

                <Button
                    android:id="@+id/date_sow_txt_btn"
                    android:layout_width="120dp"
                    android:layout_height="60dp"
                    android:layout_alignBottom="@+id/dlg_var_sow"
                    android:layout_alignParentLeft="true"
                    android:layout_alignTop="@+id/dlg_var_sow"
                    android:background="@drawable/square_btn_transp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="40dp"
                android:layout_height="60dp"
                android:layout_margin="5dip"
                android:gravity="right" >

                <ImageButton
                    android:id="@+id/date_sow_img_btn"
                    android:layout_width="40dip"
                    android:layout_height="60dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:background="@drawable/square_btn_transp"
                    android:gravity="center"
                    android:src="@drawable/date" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RelativeLayoutwf_2"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_margin="5dip"
                android:gravity="right" >

                <ImageView
                    android:id="@+id/img_bg_day_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/empty" />

                <TextView
                    android:id="@+id/dlg_lbl_day_fert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_weight="1"
                    android:text="Date"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="22sp" />

                <Button
                    android:id="@+id/home_btn_day_fert"
                    android:layout_width="120dp"
                    android:layout_height="60dp"
                    android:layout_alignBottom="@+id/dlg_var_sow"
                    android:layout_alignParentLeft="true"
                    android:layout_alignTop="@+id/dlg_var_sow"
                    android:background="@drawable/square_btn_transp" />
            </RelativeLayout>
        </TableRow>
    </LinearLayout>

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:background="@drawable/def_img" >

        <RelativeLayout
            android:id="@+id/RelativeLayoutwf_2"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="65dp"
            android:layout_marginTop="5dp" >

            <ImageView
                android:id="@+id/okbutton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:src="@drawable/ic_72px_ok" />

            <Button
                android:id="@+id/fert_ok"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_alignBottom="@+id/okbutton"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignRight="@+id/okbutton"
                android:background="@drawable/square_btn_transp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayoutwf_2"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="5dp" >

            <ImageView
                android:id="@+id/cancelbutton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:src="@drawable/ic_72px_cancel" />

            <Button
                android:id="@+id/fert_cancel"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignBottom="@+id/cancelbutton"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignRight="@+id/cancelbutton"
                android:background="@drawable/square_btn_transp" />
        </RelativeLayout>
    </TableRow>

</LinearLayout>