<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" >

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/def_img" >

        <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/linearLayout19"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="@drawable/header" >

                <ImageView
                    android:id="@+id/aggr_img_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:background="@drawable/circular_btn_normal"
                    android:src="@drawable/ic_sow" />

                <ImageView
                    android:id="@+id/aggr_img_icon"
                    android:layout_width="60dip"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:src="@drawable/plot" />

                <Button
                    android:id="@+id/button123"
                    android:layout_width="75dip"
                    android:layout_height="wrap_content"
                    android:text="Button"
                    android:visibility="invisible" />

                <ImageButton
                    android:id="@+id/aggr_img_home"
                    android:layout_width="50dip"
                    android:layout_height="match_parent"
                    android:layout_margin="3dp"
                    android:background="@drawable/home"
                    android:src="@drawable/square_btn_transp" />

                <ImageButton
                    android:id="@+id/aggr_img_help"
                    android:layout_width="50dip"
                    android:layout_height="match_parent"
                    android:layout_margin="3dp"
                    android:background="@drawable/ic_54px_help"
                    android:src="@drawable/square_btn_transp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/seed_type_sow_tr"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:paddingRight="5px"
                android:background="@android:drawable/list_selector_background"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingTop="5dp"
                android:paddingBottom="2px" >

                <TextView
                    android:id="@+id/variety_sow_txt"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:gravity="left"
                    android:text="Variety"
                    android:textColor="#000000"
                    android:textSize="20sp" />

                <ImageView
                    android:id="@+id/variety_sow_img_btn"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/variety_sow"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/dlg_var_text_sow"
                    android:layout_width="170dp"
                    android:layout_height="48dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/empty"
                    android:gravity="center_vertical|left"
                    android:paddingLeft="10px"
                    android:text="Variety"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="22sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/units_sow_tr"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@android:drawable/list_selector_background"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingRight="5px"
                android:paddingTop="5dp"
                android:paddingBottom="2px" >

                <TextView
                    android:id="@+id/amount_sow_txt"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:gravity="left"
                    android:text="Amount"
                    android:textColor="#000000"
                    android:textSize="20sp" />

                <ImageView
                    android:id="@+id/amount_sow_img_btn"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/amount"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/dlg_lbl_unit_no_sow"
                    android:layout_width="80dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/empty"
                    android:gravity="center_vertical|left"
                    android:paddingLeft="10px"
                    android:text="No"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="22sp" />
                
                <TextView
                    android:layout_width="80dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="10dp"
                    android:gravity="center_vertical|left"
                    android:text="serus"
                    android:textColor="#000000"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="22sp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/day_sow_tr"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@android:drawable/list_selector_background"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingRight="5px"
                android:paddingTop="5dp"
                android:paddingBottom="2px" >

                <TextView
                    android:id="@+id/date_sow_txt"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:gravity="left"
                    android:text="Date"
                    android:textColor="#000000"
                    android:textSize="20sp" />

                <ImageView
                    android:id="@+id/date_sow_img_btn"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/date"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/dlg_lbl_day_sow"
                    android:layout_width="80dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/empty"
                    android:gravity="center_vertical|left"
                    android:paddingLeft="10px"
                    android:text="Day"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="22sp" />

                <TextView
                    android:id="@+id/dlg_lbl_month_sow"
                    android:layout_width="85dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/empty"
                    android:gravity="center_vertical|left"
                    android:paddingLeft="10px"
                    android:text="Month"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="22sp" />
                
            </LinearLayout>

            <LinearLayout
                android:id="@+id/treatment_sow_tr"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@android:drawable/list_selector_background"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingRight="5px"
                android:paddingTop="5dp"
                android:paddingBottom="2px" >

                <TextView
                    android:id="@+id/treat_sow_txt"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:gravity="left"
                    android:text="Treatment"
                    android:textColor="#000000"
                    android:textSize="19sp"  />

                <ImageView
                    android:id="@+id/date_sow_img_btn"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/treatment"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/dlg_lbl_treat_sow"
                    android:layout_width="170dp"
                    android:layout_height="48dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/empty"
                    android:gravity="center_vertical|left"
                    android:paddingLeft="10px"
                    android:text="Treatment"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="22sp" />
                
			</LinearLayout>
			
			<LinearLayout
                android:id="@+id/intercrop_sow_tr"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@android:drawable/list_selector_background"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingTop="5dp"
                android:paddingBottom="2px" >

                <TextView
                    android:id="@+id/intercrop_sow_txt"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:gravity="left"
                    android:text="Intercrop"
                    android:textColor="#000000"
                    android:textSize="20sp" />

                <ImageView
                    android:id="@+id/treat_sow_img_btn"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/intercrop"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/dlg_lbl_intercrop_sow"
                    android:layout_width="170dp"
                    android:layout_height="48dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/empty"
                    android:gravity="center_vertical|left"
                    android:paddingLeft="10px"
                    android:text="Crop type"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="22sp" />
                
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            style="@android:style/ButtonBar"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/sow_ok"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableLeft="@drawable/ic_48px_ok"
                android:text="@string/button_ok" />

            <Button
                android:id="@+id/sow_cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableLeft="@drawable/ic_48px_cancel"
                android:text="@string/button_cancel" />
        </LinearLayout>
    </RelativeLayout>

    <ImageView
        android:id="@+id/helpIndicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_76px_sound"
        android:visibility="invisible" />

</merge>