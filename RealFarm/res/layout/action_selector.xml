<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" >

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <ScrollView
            android:id="@+id/scrollView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/dlg_img_seed"
            android:layout_marginTop="10dip" >

            <LinearLayout
                android:id="@+id/dlg_linlay_userDataDetails"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <RelativeLayout
                    android:id="@+id/RelativeLayoutwf_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="0.09" >

                    <ImageView
                        android:id="@+id/dlg_img_seed"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="20dp"
                        android:src="@drawable/ic_72px_sowing" />

                    <Button
                        android:id="@+id/btn_action_sowing"
                        android:layout_width="50dp"
                        android:layout_height="45dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/square_btn_transp" />

                    <TextView
                        android:id="@+id/dlg_lbl_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/dlg_img_seed"
                        android:layout_marginLeft="20dp"
                        android:layout_toRightOf="@+id/dlg_img_seed"
                        android:paddingBottom="2dip"
                        android:paddingTop="2dip"
                        android:text="Sowing"
                        android:textAppearance="?android:attr/textAppearanceMedium" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/RelativeLayoutwf_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="0.09" >

                    <ImageView
                        android:id="@+id/dlg_img_seed"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="20dp"
                        android:src="@drawable/ic_72px_fertilizing1" />

                    <Button
                        android:id="@+id/btn_action_fertilize"
                        android:layout_width="50dp"
                        android:layout_height="45dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/square_btn_transp" />

                    <TextView
                        android:id="@+id/dlg_lbl_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/dlg_img_seed"
                        android:layout_marginLeft="20dp"
                        android:layout_toRightOf="@+id/dlg_img_seed"
                        android:paddingBottom="2dip"
                        android:paddingTop="2dip"
                        android:text="Fertilizing"
                        android:textAppearance="?android:attr/textAppearanceMedium" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/RelativeLayoutwf_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="0.09" >

                    <ImageView
                        android:id="@+id/dlg_img_seed"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="20dp"
                        android:src="@drawable/ic_72px_spraying3" />

                    <Button
                        android:id="@+id/btn_action_spray"
                        android:layout_width="50dp"
                        android:layout_height="45dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/square_btn_transp" />

                    <TextView
                        android:id="@+id/dlg_lbl_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/dlg_img_seed"
                        android:layout_marginLeft="20dp"
                        android:layout_toRightOf="@+id/dlg_img_seed"
                        android:paddingBottom="2dip"
                        android:paddingTop="2dip"
                        android:text="Spraying"
                        android:textAppearance="?android:attr/textAppearanceMedium" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/RelativeLayoutwf_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="0.09" >

                    <ImageView
                        android:id="@+id/dlg_img_seed"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="20dp"
                        android:src="@drawable/ic_72px_reporting" />

                    <Button
                        android:id="@+id/btn_action_problem"
                        android:layout_width="50dp"
                        android:layout_height="45dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/square_btn_transp" />

                    <TextView
                        android:id="@+id/dlg_lbl_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/dlg_img_seed"
                        android:layout_marginLeft="20dp"
                        android:layout_toRightOf="@+id/dlg_img_seed"
                        android:paddingBottom="2dip"
                        android:paddingTop="2dip"
                        android:text="Problems"
                        android:textAppearance="?android:attr/textAppearanceMedium" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/RelativeLayoutwf_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="0.09" >

                    <ImageView
                        android:id="@+id/dlg_img_seed"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="20dp"
                        android:src="@drawable/ic_72px_irrigation1" />

                    <Button
                        android:id="@+id/btn_action_irrigate"
                        android:layout_width="50dp"
                        android:layout_height="45dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/square_btn_transp" />

                    <TextView
                        android:id="@+id/dlg_lbl_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/dlg_img_seed"
                        android:layout_marginLeft="20dp"
                        android:layout_toRightOf="@+id/dlg_img_seed"
                        android:paddingBottom="2dip"
                        android:paddingTop="2dip"
                        android:text="Irrigate"
                        android:textAppearance="?android:attr/textAppearanceMedium" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/RelativeLayoutwf_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="0.09" >

                    <ImageView
                        android:id="@+id/dlg_img_seed"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="20dp"
                        android:src="@drawable/ic_72px_harvesting2" />

                    <Button
                        android:id="@+id/btn_action_harvesting"
                        android:layout_width="50dp"
                        android:layout_height="45dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/square_btn_transp" />

                    <TextView
                        android:id="@+id/dlg_lbl_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/dlg_img_seed"
                        android:layout_marginLeft="20dp"
                        android:layout_toRightOf="@+id/dlg_img_seed"
                        android:paddingBottom="2dip"
                        android:paddingTop="2dip"
                        android:text="Harvest"
                        android:textAppearance="?android:attr/textAppearanceMedium" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/RelativeLayoutwf_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="0.09" >

                    <ImageView
                        android:id="@+id/dlg_img_seed"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="14dp"
                        android:src="@drawable/selling" />

                    <Button
                        android:id="@+id/btn_action_selling"
                        android:layout_width="50dp"
                        android:layout_height="45dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/square_btn_transp" />

                    <TextView
                        android:id="@+id/dlg_lbl_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/dlg_img_seed"
                        android:layout_marginLeft="18dp"
                        android:layout_toRightOf="@+id/dlg_img_seed"
                        android:paddingBottom="2dip"
                        android:paddingTop="2dip"
                        android:text="Selling"
                        android:textAppearance="?android:attr/textAppearanceMedium" />
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>

    <ImageView
        android:id="@+id/dlg_help_indicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_76px_sound"
        android:visibility="invisible" />

</merge>